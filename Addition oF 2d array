#include <iostream>
using namespace std;

int main()
{
    int i,j;
    int arr[2][2];
    int arr1[2][2];
    int sum[2][2];
    cout<<"Enter the value of first matrix";
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
        cin>>arr[i][j];
        }
    }
    cout<<"enter the value for second matrix";
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
        
        cin>>arr1[i][j];
        }
    }
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
            sum[i][j]=arr[i][j]+arr1[i][j];
            cout<<sum[i][j];
        }
    }
    return 0;
}
